<h1>Here are all the characters in <%= @book.name %></h1>

<ul>
  <% @characters.each do |character| %>
    <li><a href="/books/<%= character.book_id %>/characters/<%= character.id %>"><%= character.name %></a></li>
  <% end %>
</ul>

<p><a href="/books/<%= @book.id %>/characters/new">Add a Character</a></p>
<p><a href="/">Home</a></p>
<p><a href="/books">All Books</a></p>
